<body class="hold-transition skin-blue sidebar-mini">

<div class="wrapper">

    <!-- 页面头部 -->
    <jsp:include page="header.jsp"></jsp:include>
    <!-- 页面头部 /-->

    <!-- 导航侧栏 -->
    <jsp:include page="aside.jsp"></jsp:include>
    <!-- 导航侧栏 /-->

    <!-- 内容区域 -->
    <div class="content-wrapper">

        <!-- 内容头部 -->
        <section class="content-header">
            <h1>
                收货人详情 <small>数据列表</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
                <li><a href="#">收货人详情</a></li>
                <li class="active">数据列表</li>
            </ol>
        </section>


        <!-- 正文区域 -->
        <section class="content-header"> <!-- .box-body -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">列表</h3>
                </div>

                <div class="box-body">

                    <!-- 数据表格 -->
                    <div class="table-box">


                        <c:if test="${empty addresses}">
                            <font color="red">${msg}</font>
                        </c:if>


                        <c:if test="${not empty addresses }">
                        <!--数据列表-->
                        <table id="dataList"
                               class="table table-bordered table-striped table-hover dataTable">
                            <thead>
                            <tr>

                                <th class="sorting_asc">序号</th>
                                <th class="sorting_asc">收货人ID</th>
                                <th class="text-center">收货人名</th>
                                <th class="image">收货人电话</th>
                                <th class="number">收货人地址</th>
                            </tr>
                            </thead>
                            <tbody>
                            <c:forEach items="${addresses}" var="p" varStatus="status">

                                <tr>
                                    <td>${status.count}</td>
                                    <td>${p.addressId}</td>
                                    <td>${p.addressName}</td>
                                    <td>${p.addressPhone}</td>
                                    <td>${p.addressAddress}</td>
                                </tr>
                            </c:forEach>
                            </tbody>

                        </table>
                        <!--数据列表/-->
                        <div class="col-md-12 ">
                            <div class="box-tools text-center">
                                <button type="button" class="btn bg-default" onclick="history.back(-1);">返回</button>
                            </div>
                        </div>

                    </div>
                    <!-- 数据表格 /-->


                </div>
                <!-- /.box-body -->

                </c:if>
            </div>

        </section>
        <!-- 正文区域 /-->

    </div>

    <!-- 内容区域 /-->

</div>

